<Window x:Class="Photoer.PhotoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Photoer"
        mc:Ignorable="d"
        Title="PhotoWindow" Loaded="Window_Loaded" MouseWheel="Window_MouseWheel" KeyDown="Window_KeyDown" MouseMove="Window_MouseMove" Topmost="True">
    <Grid>
        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="Black">
            <Image x:Name="PhotoBox" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Grid>

    </Grid>
</Window>
